{
  "name": "@care-commons/family-engagement",
  "version": "0.1.0",
  "description": "Family Engagement Platform vertical for Care Commons - family portal, messaging, and AI chatbot",
  "type": "module",
  "main": "./dist/index.js",
  "engines": {
    "node": "22.x"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts"
  },
  "keywords": [
    "care-commons",
    "home-care",
    "family-engagement",
    "messaging",
    "ai-chatbot",
    "family-portal"
  ],
  "dependencies": {
    "@anthropic-ai/sdk": "^0.68.0",
    "@care-commons/core": "file:../../packages/core",
    "date-fns": "^4.1.0",
    "socket.io": "^4.8.1",
    "uuid": "^13.0.0",
    "ws": "^8.18.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitest/coverage-v8": "^4.0.7",
    "eslint": "^9.39.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^62.0.0",
    "socket.io-client": "^4.8.1",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  },
  "peerDependencies": {
    "express": "^5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  }
}
