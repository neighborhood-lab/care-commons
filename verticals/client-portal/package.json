{
  "name": "@care-commons/client-portal",
  "version": "0.1.0",
  "description": "Client self-service portal for Care Commons",
  "type": "module",
  "main": "./dist/index.js",
  "engines": {
    "node": "22.x"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext ts --max-warnings 0",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@care-commons/core": "^0.1.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "eslint": "^9.39.1",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-sonarjs": "^3.0.5",
    "eslint-plugin-unicorn": "^62.0.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.8"
  },
  "peerDependencies": {
    "express": "^4.17.0 || ^5.0.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  }
}
