{
  "name": "@care-commons/hr-onboarding",
  "version": "0.1.0",
  "description": "HR and onboarding management for care organizations",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@care-commons/core": "file:../../packages/core",
    "@care-commons/caregiver-staff": "file:../caregiver-staff",
