{
  "name": "@care-commons/data-backup-offline",
  "version": "0.1.0",
  "description": "Comprehensive backup management and offline synchronization support for care coordination platforms",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
